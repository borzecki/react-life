    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-jest": "^24.9.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "react-scripts": "^3.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
